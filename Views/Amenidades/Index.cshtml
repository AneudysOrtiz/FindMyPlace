@model IEnumerable<FindMyPlace.Models.Amenidad>

@{
    ViewBag.Title = "Amenidades";
}

<h2>Amenidades</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AmenidadId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AmenidadId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AmenidadId })
        </td>
    </tr>
}

</table>
